{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier", "prettier/react"],
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"no-unused-expressions": "off",
		"react/jsx-filename-extension": [0],
		"prettier/prettier": "error"
	}
}
